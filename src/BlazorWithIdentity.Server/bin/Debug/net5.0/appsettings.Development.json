{
  "ASPNETCORE_ENVIRONMENT": "Production",
  "ConnectionStrings": {
    "Default": "server = (localdb)\\MSSQLLocalDB; database = BlazorEdition"
  },
  "PhotoSettings": {
    "AcceptedFileTypes": [ ".jpg", ".png", ".jpeg" ],
    "MaxBytes": 10485760
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "BlazorApp1.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*"
}